<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="mapper.member">
	<select id="loginCheck" parameterType="com.web.f5.member.vo.MemberVO" resultType="int">
		select count(*) from member where memberId = #{memberId} and memberPass= #{memberPass}
	</select>
	<insert id="insert" parameterType="com.web.f5.member.vo.MemberVO">
		insert into member values
			(#{memberId} , #{memberPass},#{memberName},#{memberBirth},#{memberGender},#{memberTel},#{memberEmail},0,sysdate,0,0,0,0,0)
	</insert>
	<select id="select" parameterType="java.lang.String" resultType="com.web.f5.member.vo.MemberVO">
		select * from member where memberId=#{memberId}
	</select>
</mapper>