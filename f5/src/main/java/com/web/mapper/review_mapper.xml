<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="mapper.review">
	<select id="selectlist" parameterType="map" resultType="com.web.f5.store.vo.ReviewVO">
		select * from review where review_idx between #{Start} and #{End}
	</select>
	<insert id="insert" parameterType="com.web.f5.store.vo.ReviewVO">
			insert into review
				values(review_sequ.nextval,#{store_idx},#{member_id},#{review_content},#{review_score})
	</insert>
</mapper>
