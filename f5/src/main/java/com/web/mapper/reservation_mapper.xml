<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="mapper.reservation">
	<insert id="insert" parameterType="com.web.f5.vo.ReservationVO">
		insert into reservation values('rez_'||sequ_reservation.nextval,0,#{storeIdx},#{memberId},#{reservationCount},#{reservationDate})
	</insert>
</mapper>
